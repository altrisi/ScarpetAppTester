{
  "schemaVersion": 1,
  "id": "scarpet-app-tester",
  "version": "${version}",

  "name": "Scarpet App Automatic Tester",
  "description": "This mod will test Scarpet apps programatically, for example, inside of a Github Action, in order to check that it is working correctly.\n\nNot intended for use as a regular mod",
  "authors": [
    "altrisi"
  ],
  "contact": {
    "homepage": "https://github.com/altrisi/scarpetapptester",
    "sources": "https://github.com/altrisi/scarpetapptester.git",
    "issues": "https://github.com/altrisi/scarpetapptester/issues"
  },

  "license": "LGPL-3.0-only",
  "icon": "assets/scarpetapptester/icon.png",

  "environment": "server",
  "entrypoints": {
    "main": [
      "altrisi.scarpetapptester.ScarpetAppTester"
    ]
  },
  "mixins": [
    "scarpetapptester.mixins.json"
  ],

  "depends": {
    "minecraft": "1.16.x",
    "fabricloader": ">=0.4.0",
    "carpet": ">=1.4.14"
  },
  "suggests": {
    "another-mod": "*"
  }
}
